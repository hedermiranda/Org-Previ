    <formulas>
        <name>SAMEDAY</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(DATE(YEAR(DATEVALUE({!$Record.CreatedDate})), MONTH(DATEVALUE({!$Record.CreatedDate})), DAY(DATEVALUE({!$Record.CreatedDate})))) +0 + 21/24</expression>
    </formulas>
    <formulas>
        <name>TODAY</name>
        <dataType>Boolean</dataType>
        <expression>IF(OR(VALUE( MID( TEXT( {!$Record.CreatedDate} - 03 ), 12, 2 ) ) &lt; 21,
VALUE( MID( TEXT( {!$Record.CreatedDate} - 03 ), 12, 2 ) ) = 21, VALUE( MID( TEXT( {!$Record.CreatedDate} - 03 ), 15, 2 ) ) = 00), true, false
)</expression>
    </formulas>
    <formulas>
        <name>TODAYDATE</name>
        <dataType>Date</dataType>
        <expression>TODAY()</expression>
    </formulas>
    <formulas>
